[user]
  name = Emmanuel Etti
  email = emmanueletti@gmail.com
[core]
  editor = vim
[init]
  defaultBranch = main
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[push]
	default = current
[pull]
  ff = only
[commit]
  template = ~/.gitmessage
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
