[user]
  name = Emmanuel Etti
  email = emmanueletti@gmail.com
[core]
  editor = code --wait
[init]
  defaultBranch = main
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[alias]
  st = status
  ci = commit
  co = checkout
  di = diff
  dc = diff --cached
  amend = commit --amend
  aa = add --all :/
  ff = merge --ff-only
  pullff = pull --ff-only
  noff = merge --no-ff
  fa = fetch --all
  pom = push origin master
  b = branch
  ds = diff --stat=160,120
  dh1 = diff HEAD~1
  head = !git l -1
  h = !git head
  r = !git l -30
  ra = !git r --all
  la = !git l --all
[push]
	default = current
[pull]
  ff = only
[include]
  path = ~/.gitconfig.local
[commit]
  template = ~/.gitmessage
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
